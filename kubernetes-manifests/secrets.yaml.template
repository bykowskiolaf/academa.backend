apiVersion: v1
kind: Secret
metadata:
  name: blog-app-secrets
  namespace: blog-app
type: Opaque
data:
  .DB_USER: <base64-encoded-value>
  .DB_PASSWORD: <base64-encoded-value>
  .DB_NAME: <base64-encoded-value>
  .GOOGLE_CLIENT_ID: <base64-encoded-value>
  .GOOGLE_CLIENT_SECRET: <base64-encoded-value>
  .REDIRECT_URI: <base64-encoded-value>